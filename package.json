{
    "name": "project-opener",
    "version": "2020.2.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run start:renderer\" \"wait-on http://localhost:8080 && npm run start:electron\"",
        "start:electron": "cd src/main && tsc-watch --onSuccess \"npm run start:electron-process\"",
        "start:electron-process": "electron dist/main/app.js",
        "start:renderer": "cd src/renderer && webpack-dev-server",
        "build": "rimraf dist && npm run build:electron && npm run build:renderer",
        "build:electron": "cd src/main && tsc",
        "build:renderer": "cd src/renderer && webpack --mode production"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dgoudie/project-opener.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/dgoudie/project-opener/issues"
    },
    "homepage": "https://github.com/dgoudie/project-opener#readme",
    "devDependencies": {
        "@types/electron": "^1.6.10",
        "@types/node": "^12",
        "@types/react-dom": "^16.9.5",
        "concurrently": "^5.1.0",
        "copy-webpack-plugin": "^5.1.1",
        "electron": "^8.0.2",
        "electron-is-dev": "^1.1.0",
        "mini-css-extract-plugin": "^0.9.0",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "rimraf": "^3.0.2",
        "ts-loader": "^6.2.1",
        "tsc-watch": "^4.2.2",
        "typescript": "^3.8.3",
        "wait-on": "^4.0.1",
        "webpack": "^4.41.6",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    }
}
